<Window x:Class="PA.UI.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PasswordWindow" Height="140" Width="285">
    <Grid x:Name="MainGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="83*"/>
            <ColumnDefinition Width="28*"/>
            <ColumnDefinition Width="35*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="OkBtn" Content="Ok" HorizontalAlignment="Left" Margin="110,65,0,0" VerticalAlignment="Top" Width="75" Grid.ColumnSpan="2"/>
        <Label x:Name="PasswordLbl" Content="Password" HorizontalAlignment="Left" Margin="44,20,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="PasswordTxtbx" HorizontalAlignment="Left" Height="23" Margin="110,20,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.ColumnSpan="3" />

    </Grid>
</Window>
